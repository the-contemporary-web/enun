{
  "name": "@enun/state",
  "version": "0.2.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:fix": "prettier . --write",
    "prepack": "yarn build"
  },
  "main": "./index.ts",
  "module": "./index.ts",
  "types": "./index.ts",
  "publishConfig": {
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs"
      }
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.14.1",
    "typescript": "^5.8.3",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.3"
  },
  "dependencies": {
    "@enun/store": "workspace:^",
    "deepmerge": "^4.3.1",
    "immer": "^10.1.1",
    "nanoid": "^5.1.5"
  }
}
