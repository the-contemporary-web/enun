{
  "name": "enun",
  "packageManager": "yarn@4.5.3",
  "type": "module",
  "workspaces": [
    "demo",
    "package/*"
  ],
  "scripts": {
    "demo": "yarn workspace demo",
    "store": "yarn workspace @enun/store",
    "react": "yarn workspace @enun/react",
    "lint:check": "yarn workspaces foreach -A -p run lint:check",
    "lint:fix": "yarn workspaces foreach -A -p run lint:fix",
    "format:fix": "yarn workspaces foreach -A -p run format:fix"
  },
  "devDependencies": {
    "eslint": "^9.24.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1"
  }
}
